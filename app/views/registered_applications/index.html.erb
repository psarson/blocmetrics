<h1>Registered Applications</h1>

<div class="row">
   <div class="col-md-8">
     <% @registered_applications.each do |registered_application| %>
       <p><%= link_to registered_application.name, registered_application_path(registered_application.id) %></p>
     <% end %>
   </div>
 </div>
  <div class="col-md-4">
    <%= link_to "Register", new_registered_application_path(@user), class: 'btn btn-success' %>
  </div>
